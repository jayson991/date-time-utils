  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.3",
    "@swc/jest": "^0.2.22",
    "@types/jest": "^29.0.3",
    "@types/node": "^18.7.21",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "core-js": "^3.25.2",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
